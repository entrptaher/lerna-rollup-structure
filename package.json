{
  "name": "root",
  "private": true,
  "devDependencies": {
    "lerna": "^3.13.1"
  },
  "scripts": {
    "build:watch": "lerna exec -- rollup -w -c=rollup.config.js",
    "run:scraper": "lerna exec --scope=@tahy/scraper -- yarn run start",
    "dev": "npm-run-all --parallel build:watch run:scraper"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "workspaces": ["packages/*"]
}
